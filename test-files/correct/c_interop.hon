X :: 1
Y :: 2
Z :: X + Y

# External C functions (no body)
puts :: c func(s: i32) i32
strlen :: c func(s: i32) i32

# Simple const return value (immediate)
const_return :: c func() i32 {
  return 42
}

# Return named const value
const_return_named :: c func() i32 {
  return Z
}

# Honey function with C calling convention
my_callback :: c func(data: i32, len: i32) i32 {
  return data + len
}

# Regular Honey function
add :: func(a: i32, b: i32) i32 {
  return a + b
}

main :: func() i32 {
  return add(1 + 2, 3)
}
