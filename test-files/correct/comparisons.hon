test_equal :: func() i32 {
  if 5 == 5 { return 1 }
  return 0
}

test_not_equal :: func() i32 {
  if 5 != 3 { return 1 }
  return 0
}

test_less :: func() i32 {
  if 3 < 5 { return 1 }
  return 0
}

test_greater :: func() i32 {
  if 5 > 3 { return 1 }
  return 0
}

test_less_equal :: func() i32 {
  if 5 <= 5 { return 1 }
  return 0
}

test_greater_equal :: func() i32 {
  if 5 >= 5 { return 1 }
  return 0
}

test_signed :: func() i32 {
  mut x: i32 = 0 - 1
  if x < 0 { return 1 }
  return 0
}

test_compare_vars :: func(a: i32, b: i32) bool {
  return a < b
}

main :: func() i32 {
  mut r: i32 = 0

  r = test_equal()
  if r != 1 { return 10 }

  r = test_not_equal()
  if r != 1 { return 20 }

  r = test_less()
  if r != 1 { return 30 }

  r = test_greater()
  if r != 1 { return 40 }

  r = test_less_equal()
  if r != 1 { return 50 }

  r = test_greater_equal()
  if r != 1 { return 60 }

  r = test_signed()
  if r != 1 { return 70 }

  if test_compare_vars(3, 5) != true { return 80 }
  if test_compare_vars(5, 3) != false { return 90 }

  return 0
}
