Point :: c struct {
  x: i32,
  y: i32,
}

TwoPoints :: c struct {
  p1: Point,
  p2: Point,
}

get_x :: func(p: Point) i32 {
  return p.x
}

get_y :: func(p: Point) i32 {
  return p.y
}

main :: func() i32 {
  p := TwoPoints{
    .p1 = Point{ .x = 3, .y = 4 },
    .p2 = Point{ .x = 1, .y = 2 },
  }

  return get_y(p.p1)
}
